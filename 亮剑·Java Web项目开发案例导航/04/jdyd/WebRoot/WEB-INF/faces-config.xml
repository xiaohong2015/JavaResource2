<?xml version='1.0' encoding='GBK'?>
<!DOCTYPE faces-config PUBLIC
  "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.1//EN"
  "http://java.sun.com/dtd/web-facesconfig_1_1.dtd">
<faces-config>

    <application>
        <el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
    </application>
		<!--用户注册-->
		<managed-bean>
			<managed-bean-name>RegBean</managed-bean-name>
			<managed-bean-class>wyf.RegBean</managed-bean-class>
			<managed-bean-scope>request</managed-bean-scope>
			<managed-property>
				<property-name>gender</property-name>
				<value>男</value>
			</managed-property>
			<managed-property>
				<property-name>dbu</property-name>
				<value>#{dbu}</value>
			</managed-property>
		</managed-bean>
		<!--用户登陆-->
		<managed-bean>
			<managed-bean-name>UserLogin</managed-bean-name>
			<managed-bean-class>wyf.UserLogBean</managed-bean-class>
			<managed-bean-scope>session</managed-bean-scope>
			<managed-property>
				<property-name>dbu</property-name>
				<value>#{dbu}</value>
			</managed-property>
		</managed-bean>
		<!--用户修改密码-->
		<managed-bean>
			<managed-bean-name>UCPBean</managed-bean-name>
			<managed-bean-class>wyf.UserChangePwdBean</managed-bean-class>
			<managed-bean-scope>request</managed-bean-scope>
			<managed-property>
				<property-name>dbu</property-name>
				<value>#{dbu}</value>
			</managed-property>
			<managed-property>
				<property-name>ulb</property-name>
				<value>#{UserLogin}</value>
			</managed-property>
		</managed-bean>
		<!--用户修改资料-->
		<managed-bean>
			<managed-bean-name>UCIBean</managed-bean-name>
			<managed-bean-class>wyf.UserChangeInfoBean</managed-bean-class>
			<managed-bean-scope>session</managed-bean-scope>
			<managed-property>
				<property-name>dbu</property-name>
				<value>#{dbu}</value>
			</managed-property>
			<managed-property>
				<property-name>ulb</property-name>
				<value>#{UserLogin}</value>
			</managed-property>
		</managed-bean>
		<!--资源列表-->
		<managed-bean>
			<managed-bean-name>RLBean</managed-bean-name>
			<managed-bean-class>wyf.ResListBean</managed-bean-class>
			<managed-bean-scope>session</managed-bean-scope>
			<managed-property>
				<property-name>dbu</property-name>
				<value>#{dbu}</value>
			</managed-property>
		</managed-bean>
		<!--当前订单-->
		<managed-bean>
			<managed-bean-name>OIBean</managed-bean-name>
			<managed-bean-class>wyf.OrderBean</managed-bean-class>
			<managed-bean-scope>session</managed-bean-scope>
			<managed-property>
				<property-name>dbu</property-name>
				<value>#{dbu}</value>
			</managed-property>
			<managed-property>
				<property-name>jt</property-name>
				<value>#{jdbcTemplate}</value>
			</managed-property>
			<managed-property>
				<property-name>ulb</property-name>
				<value>#{UserLogin}</value>
			</managed-property>
		</managed-bean>
		<!--我的订单-->
		<managed-bean>
			<managed-bean-name>MOBean</managed-bean-name>
			<managed-bean-class>wyf.MyOrderBean</managed-bean-class>
			<managed-bean-scope>session</managed-bean-scope>
			<managed-property>
				<property-name>dbu</property-name>
				<value>#{dbu}</value>
			</managed-property>
			<managed-property>
				<property-name>ulb</property-name>
				<value>#{UserLogin}</value>
			</managed-property>
		</managed-bean>		
		<!--管理员登陆-->
		<managed-bean>
			<managed-bean-name>ALBean</managed-bean-name>
			<managed-bean-class>wyf.AdminLogBean</managed-bean-class>
			<managed-bean-scope>session</managed-bean-scope>
			<managed-property>
				<property-name>dbu</property-name>
				<value>#{dbu}</value>
			</managed-property>
		</managed-bean>
		<!--管理员修改密码-->
		<managed-bean>
			<managed-bean-name>ACPBean</managed-bean-name>
			<managed-bean-class>wyf.AdminChangePwdBean</managed-bean-class>
			<managed-bean-scope>request</managed-bean-scope>
			<managed-property>
				<property-name>dbu</property-name>
				<value>#{dbu}</value>
			</managed-property>
			<managed-property>
				<property-name>alb</property-name>
				<value>#{ALBean}</value>
			</managed-property>
		</managed-bean>
	<!--管理员列表-->
		<managed-bean>
			<managed-bean-name>AListBean</managed-bean-name>
			<managed-bean-class>wyf.AdminListBean</managed-bean-class>
			<managed-bean-scope>session</managed-bean-scope>
			<managed-property>
				<property-name>dbu</property-name>
				<value>#{dbu}</value>
			</managed-property>
		</managed-bean>
		
		<!--添加管理员-->
		<managed-bean>
			<managed-bean-name>AABean</managed-bean-name>
			<managed-bean-class>wyf.AddAdminBean</managed-bean-class>
			<managed-bean-scope>request</managed-bean-scope>
			<managed-property>
				<property-name>dbu</property-name>
				<value>#{dbu}</value>
			</managed-property>
		</managed-bean>
				
		<!--重置管理员密码-->
		<managed-bean>
			<managed-bean-name>RPBean</managed-bean-name>
			<managed-bean-class>wyf.ResetPwdBean</managed-bean-class>
			<managed-bean-scope>request</managed-bean-scope>
			<managed-property>
				<property-name>dbu</property-name>
				<value>#{dbu}</value>
			</managed-property>
		</managed-bean>
		
		<!--分组管理Bean-->
		<managed-bean>
			<managed-bean-name>GLBean</managed-bean-name>
			<managed-bean-class>wyf.GroupListBean</managed-bean-class>
			<managed-bean-scope>session</managed-bean-scope>
			<managed-property>
				<property-name>dbu</property-name>
				<value>#{dbu}</value>
			</managed-property>
		</managed-bean>
		
				
		<!--资源列表页面Bean-->
		<managed-bean>
			<managed-bean-name>RMBean</managed-bean-name>
			<managed-bean-class>wyf.ResManageBean</managed-bean-class>
			<managed-bean-scope>session</managed-bean-scope>
			<managed-property>
				<property-name>dbu</property-name>
				<value>#{dbu}</value>
			</managed-property>
			<managed-property>
				<property-name>erb</property-name>
				<value>#{ERBean}</value>
			</managed-property>			
		</managed-bean>
		
		<!--添加资源-->
		<managed-bean>
			<managed-bean-name>ARBean</managed-bean-name>
			<managed-bean-class>wyf.AddResBean</managed-bean-class>
			<managed-bean-scope>request</managed-bean-scope>
			<managed-property>
				<property-name>dbu</property-name>
				<value>#{dbu}</value>
			</managed-property>			
		</managed-bean>
		
		<!--编辑资源-->
		<managed-bean>
			<managed-bean-name>ERBean</managed-bean-name>
			<managed-bean-class>wyf.EditResBean</managed-bean-class>
			<managed-bean-scope>session</managed-bean-scope>
			<managed-property>
				<property-name>dbu</property-name>
				<value>#{dbu}</value>
			</managed-property>
		</managed-bean>
		
		<!--用户列表-->
		<managed-bean>
			<managed-bean-name>ULBean</managed-bean-name>
			<managed-bean-class>wyf.UserListBean</managed-bean-class>
			<managed-bean-scope>session</managed-bean-scope>
			<managed-property>
				<property-name>dbu</property-name>
				<value>#{dbu}</value>
			</managed-property>
		</managed-bean>
		
		<!--订单列表页面列表-->
		<managed-bean>
			<managed-bean-name>OLBean</managed-bean-name>
			<managed-bean-class>wyf.OrderListBean</managed-bean-class>
			<managed-bean-scope>session</managed-bean-scope>
			<managed-property>
				<property-name>dbu</property-name>
				<value>#{dbu}</value>
			</managed-property>
			<managed-property>
				<property-name>aob</property-name>
				<value>#{AOBean}</value>
			</managed-property>			
		</managed-bean>
		
		<!--订单管理页面列表-->
		<managed-bean>
			<managed-bean-name>AOBean</managed-bean-name>
			<managed-bean-class>wyf.AdminOrderBean</managed-bean-class>
			<managed-bean-scope>session</managed-bean-scope>
			<managed-property>
				<property-name>dbu</property-name>
				<value>#{dbu}</value>
			</managed-property>
		</managed-bean>
		
		
		
		<!--登陆导航规则-->
    <navigation-rule>
        <from-view-id>/login.jsp</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/index.jsp</to-view-id>            
        </navigation-case>
        <navigation-case>
            <from-outcome>toOrder</from-outcome>
            <to-view-id>/nowOrder.jsp</to-view-id>            
        </navigation-case>
        <navigation-case>
            <from-outcome>fail</from-outcome>
            <to-view-id>/login.jsp</to-view-id>            
        </navigation-case>
    </navigation-rule>
    <!---->
    <navigation-rule>
        <navigation-case>
            <from-outcome>userLogin</from-outcome>
            <to-view-id>/login.jsp</to-view-id>            
        </navigation-case>
        <navigation-case>
            <from-outcome>adminLogin</from-outcome>
            <to-view-id>/adminIndex.jsp</to-view-id>            
        </navigation-case>
        <navigation-case>
            <from-outcome>goChangeInfo</from-outcome>
            <to-view-id>/changeInfo.jsp</to-view-id>            
        </navigation-case>
        <navigation-case>
            <from-outcome>groupManage</from-outcome>
            <to-view-id>/groupManage.jsp</to-view-id>            
        </navigation-case>
        <navigation-case>
            <from-outcome>resList</from-outcome>
            <to-view-id>/resList.jsp</to-view-id>            
        </navigation-case>
        <navigation-case>
            <from-outcome>resManage</from-outcome>
            <to-view-id>/resManage.jsp</to-view-id>            
        </navigation-case>
    </navigation-rule>
    <!--提交订单页面导航规则-->
    <navigation-rule>
        <from-view-id>/nowOrder.jsp</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/nowOrder.jsp</to-view-id>            
        </navigation-case>
        <navigation-case>
            <from-outcome>LOGIN</from-outcome>
            <to-view-id>/login.jsp</to-view-id>            
        </navigation-case>
    </navigation-rule>
    <!--我的订单页面查看订单详情导航规则-->
    <navigation-rule>
        <from-view-id>/myOrder.jsp</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/orderDetail.jsp</to-view-id>            
        </navigation-case>
    </navigation-rule>
    <!--查看资源预订情况-->
   	<navigation-rule>
        <from-view-id>/resList.jsp</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/resOrderList.jsp</to-view-id>            
        </navigation-case>
    </navigation-rule>
    <!--添加分组-->
   	<navigation-rule>
        <from-view-id>/groupManage.jsp</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/groupManage.jsp</to-view-id>            
        </navigation-case>
    </navigation-rule>  
    
    <!--管理端查看资源预订情况和编辑资源动作-->
   	<navigation-rule>
        <from-view-id>/resManage.jsp</from-view-id>
        <navigation-case>
            <from-outcome>resStatus</from-outcome>
            <to-view-id>/resStatus.jsp</to-view-id>            
        </navigation-case>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/editRes.jsp</to-view-id>            
        </navigation-case>
    </navigation-rule>
    
    <!--编辑资源动作-->
   	<navigation-rule>
        <from-view-id>/orderManage.jsp</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/adminOrderDetail.jsp</to-view-id>            
        </navigation-case>
    </navigation-rule>
</faces-config>