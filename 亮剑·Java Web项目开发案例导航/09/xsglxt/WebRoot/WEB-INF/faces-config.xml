<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE faces-config PUBLIC
  "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.1//EN"
  "http://java.sun.com/dtd/web-facesconfig_1_1.dtd">
<faces-config>  
	<managed-bean>
		<managed-bean-name>AddStuBean</managed-bean-name>
		<managed-bean-class>wyf.cgq.AddStuBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>lb</property-name>
			<value>#{LoginBean}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>LoginBean</managed-bean-name>
		<managed-bean-class>wyf.cgq.LoginBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>type</property-name>
			<value>student</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>StuTreeBean</managed-bean-name>
		<managed-bean-class>wyf.cgq.StuTreeBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>lb</property-name>
			<value>#{LoginBean}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>TeacherTreeBean</managed-bean-name>
		<managed-bean-class>wyf.cgq.TeacherTreeBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>lb</property-name>
			<value>#{LoginBean}</value>
		</managed-property>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>TeacherChangePwdBean</managed-bean-name>
		<managed-bean-class>wyf.cgq.TeacherChangePwdBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>lb</property-name>
			<value>#{LoginBean}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>TeacherSearchMsgBean</managed-bean-name>
		<managed-bean-class>wyf.cgq.TeacherSearchMsgBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>TeacherSearchGradeBean</managed-bean-name>
		<managed-bean-class>wyf.cgq.TeacherSearchGradeBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>TeacherManageCourseBean</managed-bean-name>
		<managed-bean-class>wyf.cgq.TeacherManageCourseBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>lb</property-name>
			<value>#{LoginBean}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>ChosingCourseBean</managed-bean-name>
		<managed-bean-class>wyf.cgq.ChosingCourseBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>lb</property-name>
			<value>#{LoginBean}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>WriteScoreBean</managed-bean-name>
		<managed-bean-class>wyf.cgq.WriteScoreBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>lb</property-name>
			<value>#{LoginBean}</value>
		</managed-property>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>AddNewCourseBean</managed-bean-name>
		<managed-bean-class>wyf.cgq.AddNewCourseBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>lb</property-name>
			<value>#{LoginBean}</value>
		</managed-property>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>PersonalMsgBean</managed-bean-name>
		<managed-bean-class>wyf.cgq.PersonalMsgBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>lb</property-name>
			<value>#{LoginBean}</value>
		</managed-property>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>StudentChangePwdBean</managed-bean-name>
		<managed-bean-class>wyf.cgq.StudentChangePwdBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>lb</property-name>
			<value>#{LoginBean}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>StudentChoseCourseBean</managed-bean-name>
		<managed-bean-class>wyf.cgq.StudentChoseCourseBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>lb</property-name>
			<value>#{LoginBean}</value>
		</managed-property>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>CourseTableBean</managed-bean-name>
		<managed-bean-class>wyf.cgq.CourseTableBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>lb</property-name>
			<value>#{LoginBean}</value>
		</managed-property>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>LearnedCourseBean</managed-bean-name>
		<managed-bean-class>wyf.cgq.LearnedCourseBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>lb</property-name>
			<value>#{LoginBean}</value>
		</managed-property>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>FailCourseBean</managed-bean-name>
		<managed-bean-class>wyf.cgq.FailCourseBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>lb</property-name>
			<value>#{LoginBean}</value>
		</managed-property>
	</managed-bean>
	<navigation-rule>
		<from-view-id>/Login.jsp</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/Student.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>fail</from-outcome>
			<to-view-id>/LoginFail.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>teacher</from-outcome>
			<to-view-id>/Teacher.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>  
	
	<navigation-rule>
		<from-view-id>/Student.jsp</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/Student.jsp</to-view-id>
		</navigation-case>
	</navigation-rule> 
	<navigation-rule>
		<from-view-id>/Teacher.jsp</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/Teacher.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	 <navigation-rule>
		<from-view-id>/TeacherTree.jsp</from-view-id>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>/Login.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>  
	<navigation-rule>
		<from-view-id>/StuTree.jsp</from-view-id>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>/Login.jsp</to-view-id>
		</navigation-case>
	</navigation-rule> 
	<application>
        <local-config>
            <default-locale>en</default-locale>
            <supported-locale>zh_CN</supported-locale>
        </local-config>
        <message-bundle>messages</message-bundle>
    </application>
    
    
    <converter>
        <converter-id>MyConveter</converter-id>
        <converter-class>
            wyf.cgq.MyConveter
        </converter-class>
    </converter>
</faces-config>
