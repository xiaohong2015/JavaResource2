<?xml version="1.0" encoding="GBK"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping 
	package="org.hibernate.auction">

	<class name="wyf.zrk.QuestionInfo" table="questioninfo">
		<id name="q_id" type="java.lang.Integer"></id>
		<property name="q_title" not-null="true" column="`q_title`"/>
		<property name="q_content" not-null="true" column="`q_content`" />
		<property name="q_flag" not-null="true" length="2" column="`q_flag`"/>
		<property name="u_id" not-null="true" column="`u_id`" />
		<property name="q_time" not-null="true" column="`q_time`" />
	</class>	
</hibernate-mapping>